@{
    string dgtitle = ViewBag.muluName;
    string weizhi = "位置：目录管理>>";
    var sf = DateTime.Now.ToString();
}

<div id="p" class="easyui-panel" title="@weizhi@dgtitle" style="padding:5px;" data-options="fit:true, border:true ">
    <div class="easyui-tabs" id="easytab" data-options="fit:true, border:true ">
        @*待分配*@
        <div title="待评议" align="center" data-options="closable:false,url:'/xiangmuguanli/shenhe_ywzy_daifenpei?muluName=@dgtitle'" style="padding: 5px;">
        </div>

        <div title="审核通过" data-options="url:'/xiangmuguanli/shenhe_ywzy_tongguo?muluName=@dgtitle&time=@sf'" style="padding:5px;">
        </div>

        @*审核未通过*@
        <div title="审核未通过" data-options="closable:false,url:'/xiangmuguanli/shenhe_ywzy_weitongguo?muluName=@dgtitle&time=@sf'" style="padding: 5px;">
        </div>

        @*未审核*@
        <div title="未审核" data-options="closable:false,url:'/xiangmuguanli/shenhe_ywzy_weishenhe?muluName=@dgtitle&time=@sf'" style="padding: 5px;">
        </div>

        <div title="部门送审" data-options="closable:false,url:'/xiangmuguanli/shenhe_ywzy_bm_tj?muluName=@dgtitle&time=@sf'" style="padding: 5px;">
        </div>

        <div title="申报进度" data-options="closable:false,url:'/xiangmuguanli/shenhe_ywzy_tjjd?muluName=@dgtitle&time=@sf'" style="padding: 5px;">
        </div>
    </div>
</div>

@section footer{
    <script src="~/homescript/xiangmuguanli/shenhe_ywzy.js">

    </script>
}
















